let raceNumber = Math.floor(Math.random() * 1000);
let early = true;
let age = 19;
if (early = true && age > 18){
  raceNumber += 1000
}
if (early = true && age > 18)[
  console.log("You will race at 9:30. Your race numnber is: ${raceNumber}.")
]
if (early = false && age > 18)[
  console.log("Late aduls run at 11:00 am")
]
